{
    "identifier": "iansvo.SortLinesAlphanumerically",
    "name": "Sort Lines Alphanumerically",
    "organization": "Ian Svoboda",
    "description": "Adds a simple editor command to sort lines alphanumerically.",
    "version": "1.0",
    "categories": ["commands"],    
    "main": "main.js",   
    "bugs": "https://github.com/iansvo/nova-sort-lines-alphanumerically/issues", 
    "activationEvents": [],    
    "entitlements": {},
    "commands": {
        "editor": [
            {
                "title": "Sort Lines Alphanumerically",
                "command": "sort-lines-alphabetically.sortLinesAlphanumerically",
                "when": "editorHasSelection",
                "shortcut": "opt-shift-g"
            }         
        ]
    }
}
